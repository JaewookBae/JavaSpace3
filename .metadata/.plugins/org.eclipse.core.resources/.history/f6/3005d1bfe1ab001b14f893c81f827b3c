package study02;

public class OperationTest1 {
//산술 연산자 테스트
	public static void main(String[] args) {
		
		int result = 0;
		int var_int1 = 10;
		int var_int2 = 2;
		
		result = var_int1 + var_int2;
		System.out.println("var_int1 + var_int2 = " + result);
		//System.out.println("var_int1 - var_int2 = " + var_int1 - var_int2);
		System.out.println("var_int1 * var_int2 = " + var_int1 * var_int2);
		System.out.println("var_int1 / var_int2 = " + var_int1 / var_int2);
		System.out.println("var_int1 % var_int2 = " + var_int1 % var_int2);

	}

}
