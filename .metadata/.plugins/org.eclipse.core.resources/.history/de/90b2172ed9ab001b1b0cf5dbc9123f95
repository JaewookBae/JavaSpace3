package p04.binary;

public class InfinityAndNaNCheckEx {

	public static void main(String[] args) {
		
		int x = 10;
		int y = 0;
		
		try {
			int z = x/y;//몫 구하기
			int k = x%y;//나머지 구하기
			System.out.println(z);
		} catch (Exception e) {
			
			System.out.println("컴퓨터는 수학과 다릅니다.숫자를 0으로 나눌수 없습니다.");
		}

	}

}
