package p03.unary;

public class IncreaseDecreaseOperatorEx {

	public static void main(String[] args) {
		
		int x = 10;
		int y = 10;
		int z;
		
		x++;
		++x;
		//System.out.println(x++);
		System.out.println("x="+ x);
		
		y--;
		//System.out.println(y--);
		--y;
		System.out.println("y="+ y);
		
		z = x++;
		System.out.println("z="+ z);
		System.out.println("x="+ x);
	}

}
